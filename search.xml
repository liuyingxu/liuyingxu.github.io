<figure class="highlight js"><table><tr><td class="code"><pre><span class="line">Widget _listItemBuilder(BuildContext context, int index) {</span><br><span class="line">  <span class="keyword">return</span> Container(</span><br><span class="line">    color: Colors.white,</span><br><span class="line">    margin: EdgeInsets.all(<span class="number">8.0</span>),</span><br><span class="line">    child: Column(</span><br><span class="line">      children: &lt;Widget&gt;[</span><br><span class="line">        Image.network(lists[index].img),</span><br><span class="line">        SizedBox(height: 16.0),</span><br><span class="line">        Text(</span><br><span class="line">          lists[index].name,</span><br><span class="line">          style: Theme.of(context).textTheme.title</span><br><span class="line">        ),</span><br><span class="line">        Text(</span><br><span class="line">          &apos;&#x8BBF;&#x95EE;&#xFF1A;&apos; + lists[index].all_click,</span><br><span class="line">          style: Theme.of(context).textTheme.subhead</span><br><span class="line">        ),</span><br><span class="line">        Text(</span><br><span class="line">          &apos;&#x6536;&#x85CF;&#xFF1A;&apos; + lists[index].favorites,</span><br><span class="line">          style: Theme.of(context).textTheme.subhead</span><br><span class="line">        ),</span><br><span class="line">        SizedBox(height: 16.0)</span><br><span class="line">      ],</span><br><span class="line">    ),</span><br><span class="line">  );</span><br><span class="line">}</span><br></pre></td></tr></table></figure>

